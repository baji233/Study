<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ProductListServlet</servlet-name>
    <servlet-class>com.jiuyun.servlet.ProductListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddShoppingCarServlet</servlet-name>
    <servlet-class>com.jiuyun.servlet.AddShoppingCarServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RemoveShoppingServlet</servlet-name>
    <servlet-class>com.jiuyun.servlet.RemoveShoppingServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ProductListServlet</servlet-name>
    <url-pattern>/productList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddShoppingCarServlet</servlet-name>
    <url-pattern>/addShoppingCar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RemoveShoppingServlet</servlet-name>
    <url-pattern>/removeShoppingCar</url-pattern>
  </servlet-mapping>
</web-app>
